@inherits BaseComponent

<div class="ant-result ant-result-success">
    <div class="ant-result-icon">
        <span class="anticon anticon-check-circle" role="img">
            <svg focusable="false" width="1em" height="1em" fill="currentColor" style="pointer-events: none;" xmlns="http://www.w3.org/2000/svg" class="icon" viewBox="0 0 1024 1024"><path d="M512 64C264.6 64 64 264.6 64 512s200.6 448 448 448 448-200.6 448-448S759.4 64 512 64zm193.5 301.7l-210.6 292a31.8 31.8 0 0 1-51.7 0L318.5 484.9c-3.8-5.3 0-12.7 6.5-12.7h46.9c10.2 0 19.9 4.9 25.9 13.3l71.2 98.8 157.2-218c6-8.3 15.6-13.3 25.9-13.3H699c6.5 0 10.3 7.4 6.5 12.7z"></path></svg>
        </span>
    </div>
    @if (!string.IsNullOrWhiteSpace(Title))
    {
        <div class="ant-result-title">@Title</div>
    }
    @if (!string.IsNullOrWhiteSpace(SubTitle))
    {
        <div class="ant-result-subtitle">@SubTitle</div>
    }
    @if (ExtraContent != null)
    {
        <div class="ant-result-extra">
            @ExtraContent
        </div>
    }
</div>

@code {
    [Parameter] public string Type { get; set; }
    [Parameter] public string Title { get; set; }
    [Parameter] public string SubTitle { get; set; }
    [Parameter] public RenderFragment ExtraContent { get; set; }
}